<statements>
	<select id="SelectDictqclot" parameterClass="string" resultMap="DictqclotResult">SELECT DICTQCLOTID AS Dictqclotid,LOTNUM AS Lotnum,DICTQCCONTROLID AS Dictqccontrolid,QCBARCODE AS Qcbarcode,EFFECTIVEDATE AS Effectivedate,EXPIRYDATE AS Expirydate,DICTMANUFACTUREID AS Dictmanufactureid,OPERATEBY AS Operateby,CREATEDATE AS Createdate,DICTINSTRUMENTID AS Dictinstrumentid,UNIQUEID AS Uniqueid,FIRSTUSEDDATE AS Firstuseddate,QCGOAL AS Qcgoal,FAILURE AS FailureFROM DICTQCLOT<dynamic prepend="WHERE"><isParameterPresent>DICTQCLOTID = #value#</isParameterPresent></dynamic></select><insert id="InsertDictqclot" parameterClass="Dictqclot">	INSERT INTO DICTQCLOT (DICTQCLOTID,LOTNUM,DICTQCCONTROLID,QCBARCODE,EFFECTIVEDATE,EXPIRYDATE,DICTMANUFACTUREID,OPERATEBY,CREATEDATE,DICTINSTRUMENTID,UNIQUEID,FIRSTUSEDDATE,QCGOAL,FAILURE) 	VALUES(#Dictqclotid#,#Lotnum#,#Dictqccontrolid#,#Qcbarcode#,#Effectivedate:DateTime:1/1/0001 12:00:00 AM#,#Expirydate:DateTime:1/1/0001 12:00:00 AM#,#Dictmanufactureid#,#Operateby#,#Createdate:DateTime:1/1/0001 12:00:00 AM#,#Dictinstrumentid#,#Uniqueid#,#Firstuseddate:DateTime:1/1/0001 12:00:00 AM#,#Qcgoal#,#Failure#)</insert><update id="UpdateDictqclot" parameterClass="Dictqclot">UPDATE DICTQCLOT  SET DICTQCLOTID=#Dictqclotid#,LOTNUM=#Lotnum#,DICTQCCONTROLID=#Dictqccontrolid#,QCBARCODE=#Qcbarcode#,EFFECTIVEDATE=#Effectivedate#,EXPIRYDATE=#Expirydate#,DICTMANUFACTUREID=#Dictmanufactureid#,OPERATEBY=#Operateby#,CREATEDATE=#Createdate#,DICTINSTRUMENTID=#Dictinstrumentid#,UNIQUEID=#Uniqueid#,FIRSTUSEDDATE=#Firstuseddate#,QCGOAL=#Qcgoal#,FAILURE=#Failure#WHERE DICTQCLOTID = #Dictqclotid#</update><delete id="DeleteDictqclot" parameterClass="string">DELETE FROM DICTQCLOTWHERE DICTQCLOTID = #value#</delete>
