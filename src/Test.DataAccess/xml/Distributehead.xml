<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="DistributeheadMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
<alias>
	<typeAlias alias="Distributehead"  type="Daan.DataTransfer.Test.Domain.Distributehead" />
</alias>
<resultMaps>
		<resultMap id="DistributeheadResult" class="Distributehead"><result property="Rackbarcode" column="RACKBARCODE" type="string" dbType="varchar"/><result property="Dicttuberackid" column="DICTTUBERACKID" type="single" dbType="Real"/><result property="Distributeby" column="DISTRIBUTEBY" type="single" dbType="Real"/><result property="Distributedate" column="DISTRIBUTEDATE" type="DateTime" dbType="DateTime"/><result property="Handleby" column="HANDLEBY" type="single" dbType="Real"/><result property="Handledate" column="HANDLEDATE" type="DateTime" dbType="DateTime"/><result property="Receiveby" column="RECEIVEBY" type="single" dbType="Real"/><result property="Receivedate" column="RECEIVEDATE" type="DateTime" dbType="DateTime"/><result property="Status" column="STATUS" type="single" dbType="Real"/><result property="Currentpos" column="CURRENTPOS" type="single" dbType="Real"/><result property="Dictlabdeptid" column="DICTLABDEPTID" type="single" dbType="Real"/></resultMap>
</resultMaps>
<statements>
	<select id="SelectDistributehead" parameterClass="string" resultMap="DistributeheadResult">SELECT RACKBARCODE AS Rackbarcode,DICTTUBERACKID AS Dicttuberackid,DISTRIBUTEBY AS Distributeby,DISTRIBUTEDATE AS Distributedate,HANDLEBY AS Handleby,HANDLEDATE AS Handledate,RECEIVEBY AS Receiveby,RECEIVEDATE AS Receivedate,STATUS AS Status,CURRENTPOS AS Currentpos,DICTLABDEPTID AS DictlabdeptidFROM DISTRIBUTEHEAD<dynamic prepend="WHERE"><isParameterPresent>RACKBARCODE = #value#</isParameterPresent></dynamic></select><insert id="InsertDistributehead" parameterClass="Distributehead">	INSERT INTO DISTRIBUTEHEAD (RACKBARCODE,DICTTUBERACKID,DISTRIBUTEBY,DISTRIBUTEDATE,HANDLEBY,HANDLEDATE,RECEIVEBY,RECEIVEDATE,STATUS,CURRENTPOS,DICTLABDEPTID) 	VALUES(#Rackbarcode#,#Dicttuberackid#,#Distributeby#,#Distributedate:DateTime:1/1/0001 12:00:00 AM#,#Handleby#,#Handledate:DateTime:1/1/0001 12:00:00 AM#,#Receiveby#,#Receivedate:DateTime:1/1/0001 12:00:00 AM#,#Status#,#Currentpos#,#Dictlabdeptid#)</insert><update id="UpdateDistributehead" parameterClass="Distributehead">UPDATE DISTRIBUTEHEAD  SET RACKBARCODE=#Rackbarcode#,DICTTUBERACKID=#Dicttuberackid#,DISTRIBUTEBY=#Distributeby#,DISTRIBUTEDATE=#Distributedate#,HANDLEBY=#Handleby#,HANDLEDATE=#Handledate#,RECEIVEBY=#Receiveby#,RECEIVEDATE=#Receivedate#,STATUS=#Status#,CURRENTPOS=#Currentpos#,DICTLABDEPTID=#Dictlabdeptid#WHERE RACKBARCODE = #Rackbarcode#</update><delete id="DeleteDistributehead" parameterClass="string">DELETE FROM DISTRIBUTEHEADWHERE RACKBARCODE = #value#</delete>
</statements>
</sqlMap>
