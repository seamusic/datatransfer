<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="PathologyresultMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
<alias>
	<typeAlias alias="Pathologyresult"  type="Daan.DataTransfer.Test.Domain.Pathologyresult" />
</alias>
<resultMaps>
		<resultMap id="PathologyresultResult" class="Pathologyresult"><result property="Pathologyresultid" column="PATHOLOGYRESULTID" type="single" dbType="Real"/><result property="Subbarcode" column="SUBBARCODE" type="string" dbType="varchar"/><result property="Dictinputtemplateitemid" column="DICTINPUTTEMPLATEITEMID" type="single" dbType="Real"/><result property="Templatename" column="TEMPLATENAME" type="string" dbType="varchar"/><result property="Parentid" column="PARENTID" type="single" dbType="Real"/><result property="Treelevel" column="TREELEVEL" type="single" dbType="Real"/><result property="Itemname" column="ITEMNAME" type="string" dbType="varchar"/><result property="Result" column="RESULT" type="string" dbType="varchar"/><result property="Displayorder" column="DISPLAYORDER" type="single" dbType="Real"/><result property="Createdate" column="CREATEDATE" type="DateTime" dbType="DateTime"/><result property="Report" column="REPORT" type="string" dbType="varchar"/><result property="Itemcode" column="ITEMCODE" type="string" dbType="varchar"/><result property="Dicttestitemid" column="DICTTESTITEMID" type="single" dbType="Real"/><result property="Transed" column="TRANSED" type="string" dbType="varchar"/></resultMap>
</resultMaps>
<statements>
	<select id="SelectPathologyresult" parameterClass="string" resultMap="PathologyresultResult">SELECT PATHOLOGYRESULTID AS Pathologyresultid,SUBBARCODE AS Subbarcode,DICTINPUTTEMPLATEITEMID AS Dictinputtemplateitemid,TEMPLATENAME AS Templatename,PARENTID AS Parentid,TREELEVEL AS Treelevel,ITEMNAME AS Itemname,RESULT AS Result,DISPLAYORDER AS Displayorder,CREATEDATE AS Createdate,REPORT AS Report,ITEMCODE AS Itemcode,DICTTESTITEMID AS Dicttestitemid,TRANSED AS TransedFROM PATHOLOGYRESULT<dynamic prepend="WHERE"><isParameterPresent>PATHOLOGYRESULTID = #value#</isParameterPresent></dynamic></select><insert id="InsertPathologyresult" parameterClass="Pathologyresult">	INSERT INTO PATHOLOGYRESULT (PATHOLOGYRESULTID,SUBBARCODE,DICTINPUTTEMPLATEITEMID,TEMPLATENAME,PARENTID,TREELEVEL,ITEMNAME,RESULT,DISPLAYORDER,CREATEDATE,REPORT,ITEMCODE,DICTTESTITEMID,TRANSED) 	VALUES(#Pathologyresultid#,#Subbarcode#,#Dictinputtemplateitemid#,#Templatename#,#Parentid#,#Treelevel#,#Itemname#,#Result#,#Displayorder#,#Createdate:DateTime:1/1/0001 12:00:00 AM#,#Report#,#Itemcode#,#Dicttestitemid#,#Transed#)</insert><update id="UpdatePathologyresult" parameterClass="Pathologyresult">UPDATE PATHOLOGYRESULT  SET PATHOLOGYRESULTID=#Pathologyresultid#,SUBBARCODE=#Subbarcode#,DICTINPUTTEMPLATEITEMID=#Dictinputtemplateitemid#,TEMPLATENAME=#Templatename#,PARENTID=#Parentid#,TREELEVEL=#Treelevel#,ITEMNAME=#Itemname#,RESULT=#Result#,DISPLAYORDER=#Displayorder#,CREATEDATE=#Createdate#,REPORT=#Report#,ITEMCODE=#Itemcode#,DICTTESTITEMID=#Dicttestitemid#,TRANSED=#Transed#WHERE PATHOLOGYRESULTID = #Pathologyresultid#</update><delete id="DeletePathologyresult" parameterClass="string">DELETE FROM PATHOLOGYRESULTWHERE PATHOLOGYRESULTID = #value#</delete>
</statements>
</sqlMap>
