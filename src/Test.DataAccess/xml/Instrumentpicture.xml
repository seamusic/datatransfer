<statements>
	<select id="SelectInstrumentpicture" parameterClass="string" resultMap="InstrumentpictureResult">SELECT INSTRUMENTPICTUREID AS Instrumentpictureid,SUBBARCODE AS Subbarcode,BATCHNO AS Batchno,FILENAME AS Filename,DISPLAYORDER AS Displayorder,CREATEDATE AS Createdate,PICTYPE AS Pictype,ISUSE AS Isuse,PICBINARY AS Picbinary,IMAGEDOTVALUE AS Imagedotvalue,ISREPORT AS Isreport,SEQ AS Seq,INSTRUMENTSEQID AS InstrumentseqidFROM INSTRUMENTPICTURE<dynamic prepend="WHERE"><isParameterPresent>INSTRUMENTPICTUREID = #value#</isParameterPresent></dynamic></select><insert id="InsertInstrumentpicture" parameterClass="Instrumentpicture">	INSERT INTO INSTRUMENTPICTURE (INSTRUMENTPICTUREID,SUBBARCODE,BATCHNO,FILENAME,DISPLAYORDER,CREATEDATE,PICTYPE,ISUSE,PICBINARY,IMAGEDOTVALUE,ISREPORT,SEQ,INSTRUMENTSEQID) 	VALUES(#Instrumentpictureid#,#Subbarcode#,#Batchno#,#Filename#,#Displayorder#,#Createdate:DateTime:1/1/0001 12:00:00 AM#,#Pictype#,#Isuse#,#Picbinary#,#Imagedotvalue#,#Isreport#,#Seq#,#Instrumentseqid#)</insert><update id="UpdateInstrumentpicture" parameterClass="Instrumentpicture">UPDATE INSTRUMENTPICTURE  SET INSTRUMENTPICTUREID=#Instrumentpictureid#,SUBBARCODE=#Subbarcode#,BATCHNO=#Batchno#,FILENAME=#Filename#,DISPLAYORDER=#Displayorder#,CREATEDATE=#Createdate#,PICTYPE=#Pictype#,ISUSE=#Isuse#,PICBINARY=#Picbinary#,IMAGEDOTVALUE=#Imagedotvalue#,ISREPORT=#Isreport#,SEQ=#Seq#,INSTRUMENTSEQID=#Instrumentseqid#WHERE INSTRUMENTPICTUREID = #Instrumentpictureid#</update><delete id="DeleteInstrumentpicture" parameterClass="string">DELETE FROM INSTRUMENTPICTUREWHERE INSTRUMENTPICTUREID = #value#</delete>
</statements>
