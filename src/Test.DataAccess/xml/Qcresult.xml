<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="QcresultMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
<alias>
	<typeAlias alias="Qcresult"  type="Daan.DataTransfer.Test.Domain.Qcresult" />
</alias>
<resultMaps>
		<resultMap id="QcresultResult" class="Qcresult"><result property="Qcresultid" column="QCRESULTID" type="single" dbType="Real"/><result property="Dicttestitemid" column="DICTTESTITEMID" type="single" dbType="Real"/><result property="Dictqclotid" column="DICTQCLOTID" type="single" dbType="Real"/><result property="Resultdate" column="RESULTDATE" type="DateTime" dbType="DateTime"/><result property="Result" column="RESULT" type="string" dbType="varchar"/><result property="Resultcomment" column="RESULTCOMMENT" type="string" dbType="varchar"/><result property="Resulttype" column="RESULTTYPE" type="string" dbType="varchar"/><result property="Status" column="STATUS" type="string" dbType="varchar"/><result property="Breakrule" column="BREAKRULE" type="string" dbType="varchar"/><result property="Rulestatus" column="RULESTATUS" type="string" dbType="varchar"/><result property="Operateby" column="OPERATEBY" type="single" dbType="Real"/><result property="Authorizeby" column="AUTHORIZEBY" type="single" dbType="Real"/><result property="Authorizedate" column="AUTHORIZEDATE" type="DateTime" dbType="DateTime"/><result property="Createdate" column="CREATEDATE" type="DateTime" dbType="DateTime"/><result property="Fixcomment" column="FIXCOMMENT" type="string" dbType="varchar"/><result property="Isaccept" column="ISACCEPT" type="string" dbType="varchar"/></resultMap>
</resultMaps>
<statements>
	<select id="SelectQcresult" parameterClass="string" resultMap="QcresultResult">SELECT QCRESULTID AS Qcresultid,DICTTESTITEMID AS Dicttestitemid,DICTQCLOTID AS Dictqclotid,RESULTDATE AS Resultdate,RESULT AS Result,RESULTCOMMENT AS Resultcomment,RESULTTYPE AS Resulttype,STATUS AS Status,BREAKRULE AS Breakrule,RULESTATUS AS Rulestatus,OPERATEBY AS Operateby,AUTHORIZEBY AS Authorizeby,AUTHORIZEDATE AS Authorizedate,CREATEDATE AS Createdate,FIXCOMMENT AS Fixcomment,ISACCEPT AS IsacceptFROM QCRESULT<dynamic prepend="WHERE"><isParameterPresent>QCRESULTID = #value#</isParameterPresent></dynamic></select><insert id="InsertQcresult" parameterClass="Qcresult">	INSERT INTO QCRESULT (QCRESULTID,DICTTESTITEMID,DICTQCLOTID,RESULTDATE,RESULT,RESULTCOMMENT,RESULTTYPE,STATUS,BREAKRULE,RULESTATUS,OPERATEBY,AUTHORIZEBY,AUTHORIZEDATE,CREATEDATE,FIXCOMMENT,ISACCEPT) 	VALUES(#Qcresultid#,#Dicttestitemid#,#Dictqclotid#,#Resultdate:DateTime:1/1/0001 12:00:00 AM#,#Result#,#Resultcomment#,#Resulttype#,#Status#,#Breakrule#,#Rulestatus#,#Operateby#,#Authorizeby#,#Authorizedate:DateTime:1/1/0001 12:00:00 AM#,#Createdate:DateTime:1/1/0001 12:00:00 AM#,#Fixcomment#,#Isaccept#)</insert><update id="UpdateQcresult" parameterClass="Qcresult">UPDATE QCRESULT  SET QCRESULTID=#Qcresultid#,DICTTESTITEMID=#Dicttestitemid#,DICTQCLOTID=#Dictqclotid#,RESULTDATE=#Resultdate#,RESULT=#Result#,RESULTCOMMENT=#Resultcomment#,RESULTTYPE=#Resulttype#,STATUS=#Status#,BREAKRULE=#Breakrule#,RULESTATUS=#Rulestatus#,OPERATEBY=#Operateby#,AUTHORIZEBY=#Authorizeby#,AUTHORIZEDATE=#Authorizedate#,CREATEDATE=#Createdate#,FIXCOMMENT=#Fixcomment#,ISACCEPT=#Isaccept#WHERE QCRESULTID = #Qcresultid#</update><delete id="DeleteQcresult" parameterClass="string">DELETE FROM QCRESULTWHERE QCRESULTID = #value#</delete>
</statements>
</sqlMap>
