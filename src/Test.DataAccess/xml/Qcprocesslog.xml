<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="QcprocesslogMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
<alias>
	<typeAlias alias="Qcprocesslog"  type="Daan.DataTransfer.Test.Domain.Qcprocesslog" />
</alias>
<resultMaps>
		<resultMap id="QcprocesslogResult" class="Qcprocesslog"><result property="Qcprocesslogid" column="QCPROCESSLOGID" type="single" dbType="Real"/><result property="Qcresultid" column="QCRESULTID" type="single" dbType="Real"/><result property="Outrules" column="OUTRULES" type="string" dbType="varchar"/><result property="Reason" column="REASON" type="string" dbType="varchar"/><result property="Processnote" column="PROCESSNOTE" type="string" dbType="varchar"/><result property="Processresult" column="PROCESSRESULT" type="string" dbType="varchar"/><result property="Processby" column="PROCESSBY" type="single" dbType="Real"/><result property="Processdate" column="PROCESSDATE" type="DateTime" dbType="DateTime"/><result property="Auditby" column="AUDITBY" type="single" dbType="Real"/><result property="Auditdate" column="AUDITDATE" type="DateTime" dbType="DateTime"/><result property="Status" column="STATUS" type="string" dbType="varchar"/></resultMap>
</resultMaps>
<statements>
	<select id="SelectQcprocesslog" parameterClass="string" resultMap="QcprocesslogResult">SELECT QCPROCESSLOGID AS Qcprocesslogid,QCRESULTID AS Qcresultid,OUTRULES AS Outrules,REASON AS Reason,PROCESSNOTE AS Processnote,PROCESSRESULT AS Processresult,PROCESSBY AS Processby,PROCESSDATE AS Processdate,AUDITBY AS Auditby,AUDITDATE AS Auditdate,STATUS AS StatusFROM QCPROCESSLOG<dynamic prepend="WHERE"><isParameterPresent>QCPROCESSLOGID = #value#</isParameterPresent></dynamic></select><insert id="InsertQcprocesslog" parameterClass="Qcprocesslog">	INSERT INTO QCPROCESSLOG (QCPROCESSLOGID,QCRESULTID,OUTRULES,REASON,PROCESSNOTE,PROCESSRESULT,PROCESSBY,PROCESSDATE,AUDITBY,AUDITDATE,STATUS) 	VALUES(#Qcprocesslogid#,#Qcresultid#,#Outrules#,#Reason#,#Processnote#,#Processresult#,#Processby#,#Processdate:DateTime:1/1/0001 12:00:00 AM#,#Auditby#,#Auditdate:DateTime:1/1/0001 12:00:00 AM#,#Status#)</insert><update id="UpdateQcprocesslog" parameterClass="Qcprocesslog">UPDATE QCPROCESSLOG  SET QCPROCESSLOGID=#Qcprocesslogid#,QCRESULTID=#Qcresultid#,OUTRULES=#Outrules#,REASON=#Reason#,PROCESSNOTE=#Processnote#,PROCESSRESULT=#Processresult#,PROCESSBY=#Processby#,PROCESSDATE=#Processdate#,AUDITBY=#Auditby#,AUDITDATE=#Auditdate#,STATUS=#Status#WHERE QCPROCESSLOGID = #Qcprocesslogid#</update><delete id="DeleteQcprocesslog" parameterClass="string">DELETE FROM QCPROCESSLOGWHERE QCPROCESSLOGID = #value#</delete>
</statements>
