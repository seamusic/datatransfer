<statements>
	<select id="SelectSpecimenresult" parameterClass="string" resultMap="SpecimenresultResult">SELECT SUBBARCODE AS Subbarcode,DICTTESTITEMID AS Dicttestitemid,RESULT AS Result,RESULTCOMMENT AS Resultcomment,UNIT AS Unit,PRECISION AS Precision,DISPLAYORDER AS Displayorder,HLFLAG AS Hlflag,PANICFLAG AS Panicflag,DELTAFLAG AS Deltaflag,REFLOW AS Reflow,REFHIGH AS Refhigh,REFMETHOD AS Refmethod,TEXTSHOW AS Textshow,PANICLOW AS Paniclow,PANICHIGH AS Panichigh,STATUS AS Status,REPORT AS Report,CREATEDATE AS Createdate,DICTINSTRUMENTID AS Dictinstrumentid,REMARK AS Remark,SPECIMENRESULTID AS Specimenresultid,ENGTEXTSHOW AS Engtextshow,ENGREMARK AS Engremark,SPECIMENREPORTID AS Specimenreportid,TRANSED AS TransedFROM SPECIMENRESULT<dynamic prepend="WHERE"><isParameterPresent>SPECIMENRESULTID = #value#</isParameterPresent></dynamic></select><insert id="InsertSpecimenresult" parameterClass="Specimenresult">	INSERT INTO SPECIMENRESULT (SUBBARCODE,DICTTESTITEMID,RESULT,RESULTCOMMENT,UNIT,PRECISION,DISPLAYORDER,HLFLAG,PANICFLAG,DELTAFLAG,REFLOW,REFHIGH,REFMETHOD,TEXTSHOW,PANICLOW,PANICHIGH,STATUS,REPORT,CREATEDATE,DICTINSTRUMENTID,REMARK,SPECIMENRESULTID,ENGTEXTSHOW,ENGREMARK,SPECIMENREPORTID,TRANSED) 	VALUES(#Subbarcode#,#Dicttestitemid#,#Result#,#Resultcomment#,#Unit#,#Precision#,#Displayorder#,#Hlflag#,#Panicflag#,#Deltaflag#,#Reflow#,#Refhigh#,#Refmethod#,#Textshow#,#Paniclow#,#Panichigh#,#Status#,#Report#,#Createdate:DateTime:1/1/0001 12:00:00 AM#,#Dictinstrumentid#,#Remark#,#Specimenresultid#,#Engtextshow#,#Engremark#,#Specimenreportid#,#Transed#)</insert><update id="UpdateSpecimenresult" parameterClass="Specimenresult">UPDATE SPECIMENRESULT  SET SUBBARCODE=#Subbarcode#,DICTTESTITEMID=#Dicttestitemid#,RESULT=#Result#,RESULTCOMMENT=#Resultcomment#,UNIT=#Unit#,PRECISION=#Precision#,DISPLAYORDER=#Displayorder#,HLFLAG=#Hlflag#,PANICFLAG=#Panicflag#,DELTAFLAG=#Deltaflag#,REFLOW=#Reflow#,REFHIGH=#Refhigh#,REFMETHOD=#Refmethod#,TEXTSHOW=#Textshow#,PANICLOW=#Paniclow#,PANICHIGH=#Panichigh#,STATUS=#Status#,REPORT=#Report#,CREATEDATE=#Createdate#,DICTINSTRUMENTID=#Dictinstrumentid#,REMARK=#Remark#,SPECIMENRESULTID=#Specimenresultid#,ENGTEXTSHOW=#Engtextshow#,ENGREMARK=#Engremark#,SPECIMENREPORTID=#Specimenreportid#,TRANSED=#Transed#WHERE SPECIMENRESULTID = #Specimenresultid#</update><delete id="DeleteSpecimenresult" parameterClass="string">DELETE FROM SPECIMENRESULTWHERE SPECIMENRESULTID = #value#</delete>
</statements>
