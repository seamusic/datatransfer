<statements>
	<select id="SelectInstrumentresult" parameterClass="string" resultMap="InstrumentresultResult">SELECT INSTRUMENTRESULTID AS Instrumentresultid,DICTINSTRUMENTID AS Dictinstrumentid,SUBBARCODE AS Subbarcode,DICTTESTITEMID AS Dicttestitemid,BATCHNO AS Batchno,TESTDATE AS Testdate,SEQ AS Seq,SACKNO AS Sackno,TRAYNO AS Trayno,TESTFLAG AS Testflag,RESULT AS Result,ISUSE AS Isuse,CREATEDATE AS CreatedateFROM INSTRUMENTRESULT<dynamic prepend="WHERE"><isParameterPresent>INSTRUMENTRESULTID = #value#</isParameterPresent></dynamic></select><insert id="InsertInstrumentresult" parameterClass="Instrumentresult">	INSERT INTO INSTRUMENTRESULT (INSTRUMENTRESULTID,DICTINSTRUMENTID,SUBBARCODE,DICTTESTITEMID,BATCHNO,TESTDATE,SEQ,SACKNO,TRAYNO,TESTFLAG,RESULT,ISUSE,CREATEDATE) 	VALUES(#Instrumentresultid#,#Dictinstrumentid#,#Subbarcode#,#Dicttestitemid#,#Batchno#,#Testdate:DateTime:1/1/0001 12:00:00 AM#,#Seq#,#Sackno#,#Trayno#,#Testflag#,#Result#,#Isuse#,#Createdate:DateTime:1/1/0001 12:00:00 AM#)</insert><update id="UpdateInstrumentresult" parameterClass="Instrumentresult">UPDATE INSTRUMENTRESULT  SET INSTRUMENTRESULTID=#Instrumentresultid#,DICTINSTRUMENTID=#Dictinstrumentid#,SUBBARCODE=#Subbarcode#,DICTTESTITEMID=#Dicttestitemid#,BATCHNO=#Batchno#,TESTDATE=#Testdate#,SEQ=#Seq#,SACKNO=#Sackno#,TRAYNO=#Trayno#,TESTFLAG=#Testflag#,RESULT=#Result#,ISUSE=#Isuse#,CREATEDATE=#Createdate#WHERE INSTRUMENTRESULTID = #Instrumentresultid#</update><delete id="DeleteInstrumentresult" parameterClass="string">DELETE FROM INSTRUMENTRESULTWHERE INSTRUMENTRESULTID = #value#</delete>
</statements>
