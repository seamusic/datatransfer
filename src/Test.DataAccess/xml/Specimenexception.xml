<statements>
	<select id="SelectSpecimenexception" parameterClass="string" resultMap="SpecimenexceptionResult">SELECT SPECIMENEXCEPTIONID AS Specimenexceptionid,EXCEPTIONTYPE AS Exceptiontype,SUBBARCODE AS Subbarcode,APPLYBY AS Applyby,APPLYDATE AS Applydate,REMARK AS Remark,APPROVEBY AS Approveby,APPROVEDATE AS Approvedate,SENDBY AS Sendby,TAKEBY AS Takeby,TAKEDATE AS Takedate,DELAYDATE AS Delaydate,PRINTBY AS Printby,PRINTDATE AS Printdate,STATUS AS Status,CREATEDATE AS Createdate,BARCODE AS Barcode,PDFFLAG AS Pdfflag,TESTNAMES AS Testnames,DICTLABDEPTID AS DictlabdeptidFROM SPECIMENEXCEPTION<dynamic prepend="WHERE"><isParameterPresent>SPECIMENEXCEPTIONID = #value#</isParameterPresent></dynamic></select><insert id="InsertSpecimenexception" parameterClass="Specimenexception">	INSERT INTO SPECIMENEXCEPTION (SPECIMENEXCEPTIONID,EXCEPTIONTYPE,SUBBARCODE,APPLYBY,APPLYDATE,REMARK,APPROVEBY,APPROVEDATE,SENDBY,TAKEBY,TAKEDATE,DELAYDATE,PRINTBY,PRINTDATE,STATUS,CREATEDATE,BARCODE,PDFFLAG,TESTNAMES,DICTLABDEPTID) 	VALUES(#Specimenexceptionid#,#Exceptiontype#,#Subbarcode#,#Applyby#,#Applydate:DateTime:1/1/0001 12:00:00 AM#,#Remark#,#Approveby#,#Approvedate:DateTime:1/1/0001 12:00:00 AM#,#Sendby#,#Takeby#,#Takedate:DateTime:1/1/0001 12:00:00 AM#,#Delaydate:DateTime:1/1/0001 12:00:00 AM#,#Printby#,#Printdate:DateTime:1/1/0001 12:00:00 AM#,#Status#,#Createdate:DateTime:1/1/0001 12:00:00 AM#,#Barcode#,#Pdfflag#,#Testnames#,#Dictlabdeptid#)</insert><update id="UpdateSpecimenexception" parameterClass="Specimenexception">UPDATE SPECIMENEXCEPTION  SET SPECIMENEXCEPTIONID=#Specimenexceptionid#,EXCEPTIONTYPE=#Exceptiontype#,SUBBARCODE=#Subbarcode#,APPLYBY=#Applyby#,APPLYDATE=#Applydate#,REMARK=#Remark#,APPROVEBY=#Approveby#,APPROVEDATE=#Approvedate#,SENDBY=#Sendby#,TAKEBY=#Takeby#,TAKEDATE=#Takedate#,DELAYDATE=#Delaydate#,PRINTBY=#Printby#,PRINTDATE=#Printdate#,STATUS=#Status#,CREATEDATE=#Createdate#,BARCODE=#Barcode#,PDFFLAG=#Pdfflag#,TESTNAMES=#Testnames#,DICTLABDEPTID=#Dictlabdeptid#WHERE SPECIMENEXCEPTIONID = #Specimenexceptionid#</update><delete id="DeleteSpecimenexception" parameterClass="string">DELETE FROM SPECIMENEXCEPTIONWHERE SPECIMENEXCEPTIONID = #value#</delete>
