<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="DicthostMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
<alias>
	<typeAlias alias="Dicthost"  type="Daan.DataTransfer.Test.Domain.Dicthost" />
</alias>
<resultMaps>
		<resultMap id="DicthostResult" class="Dicthost"><result property="Dicthostid" column="DICTHOSTID" type="single" dbType="Real"/><result property="Hostname" column="HOSTNAME" type="string" dbType="varchar"/><result property="Hostdesc" column="HOSTDESC" type="string" dbType="varchar"/><result property="Hosttype" column="HOSTTYPE" type="string" dbType="varchar"/><result property="Connecttype" column="CONNECTTYPE" type="string" dbType="varchar"/><result property="Initdir" column="INITDIR" type="string" dbType="varchar"/><result property="Localdir" column="LOCALDIR" type="string" dbType="varchar"/><result property="Hostport" column="HOSTPORT" type="string" dbType="varchar"/><result property="Isssl" column="ISSSL" type="string" dbType="varchar"/><result property="Username" column="USERNAME" type="string" dbType="varchar"/><result property="Password" column="PASSWORD" type="string" dbType="varchar"/><result property="Fristorder" column="FRISTORDER" type="single" dbType="Real"/></resultMap>
</resultMaps>
<statements>
	<select id="SelectDicthost" parameterClass="string" resultMap="DicthostResult">SELECT DICTHOSTID AS Dicthostid,HOSTNAME AS Hostname,HOSTDESC AS Hostdesc,HOSTTYPE AS Hosttype,CONNECTTYPE AS Connecttype,INITDIR AS Initdir,LOCALDIR AS Localdir,HOSTPORT AS Hostport,ISSSL AS Isssl,USERNAME AS Username,PASSWORD AS Password,FRISTORDER AS FristorderFROM DICTHOST<dynamic prepend="WHERE"><isParameterPresent>DICTHOSTID = #value#</isParameterPresent></dynamic></select><insert id="InsertDicthost" parameterClass="Dicthost">	INSERT INTO DICTHOST (DICTHOSTID,HOSTNAME,HOSTDESC,HOSTTYPE,CONNECTTYPE,INITDIR,LOCALDIR,HOSTPORT,ISSSL,USERNAME,PASSWORD,FRISTORDER) 	VALUES(#Dicthostid#,#Hostname#,#Hostdesc#,#Hosttype#,#Connecttype#,#Initdir#,#Localdir#,#Hostport#,#Isssl#,#Username#,#Password#,#Fristorder#)</insert><update id="UpdateDicthost" parameterClass="Dicthost">UPDATE DICTHOST  SET DICTHOSTID=#Dicthostid#,HOSTNAME=#Hostname#,HOSTDESC=#Hostdesc#,HOSTTYPE=#Hosttype#,CONNECTTYPE=#Connecttype#,INITDIR=#Initdir#,LOCALDIR=#Localdir#,HOSTPORT=#Hostport#,ISSSL=#Isssl#,USERNAME=#Username#,PASSWORD=#Password#,FRISTORDER=#Fristorder#WHERE DICTHOSTID = #Dicthostid#</update><delete id="DeleteDicthost" parameterClass="string">DELETE FROM DICTHOSTWHERE DICTHOSTID = #value#</delete>
</statements>
</sqlMap>
