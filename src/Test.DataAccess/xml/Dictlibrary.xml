<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="DictlibraryMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
<alias>
	<typeAlias alias="Dictlibrary"  type="Daan.DataTransfer.Test.Domain.Dictlibrary" />
</alias>
<resultMaps>
		<resultMap id="DictlibraryResult" class="Dictlibrary"><result property="Dictlibraryid" column="DICTLIBRARYID" type="single" dbType="Real"/><result property="Librarytypecode" column="LIBRARYTYPECODE" type="string" dbType="varchar"/><result property="Librarytypename" column="LIBRARYTYPENAME" type="string" dbType="varchar"/><result property="Fastcode" column="FASTCODE" type="string" dbType="varchar"/><result property="Active" column="ACTIVE" type="string" dbType="varchar"/><result property="Displayorder" column="DISPLAYORDER" type="single" dbType="Real"/><result property="Libraryname" column="LIBRARYNAME" type="string" dbType="varchar"/><result property="Remark" column="REMARK" type="string" dbType="varchar"/><result property="Englibraryname" column="ENGLIBRARYNAME" type="string" dbType="varchar"/><result property="Lastupdatedate" column="LASTUPDATEDATE" type="DateTime" dbType="DateTime"/><result property="Uniqueid" column="UNIQUEID" type="string" dbType="varchar"/></resultMap>
</resultMaps>
<statements>
	<select id="SelectDictlibrary" parameterClass="string" resultMap="DictlibraryResult">SELECT DICTLIBRARYID AS Dictlibraryid,LIBRARYTYPECODE AS Librarytypecode,LIBRARYTYPENAME AS Librarytypename,FASTCODE AS Fastcode,ACTIVE AS Active,DISPLAYORDER AS Displayorder,LIBRARYNAME AS Libraryname,REMARK AS Remark,ENGLIBRARYNAME AS Englibraryname,LASTUPDATEDATE AS Lastupdatedate,UNIQUEID AS UniqueidFROM DICTLIBRARY<dynamic prepend="WHERE"><isParameterPresent>DICTLIBRARYID = #value#</isParameterPresent></dynamic></select><insert id="InsertDictlibrary" parameterClass="Dictlibrary">	INSERT INTO DICTLIBRARY (DICTLIBRARYID,LIBRARYTYPECODE,LIBRARYTYPENAME,FASTCODE,ACTIVE,DISPLAYORDER,LIBRARYNAME,REMARK,ENGLIBRARYNAME,LASTUPDATEDATE,UNIQUEID) 	VALUES(#Dictlibraryid#,#Librarytypecode#,#Librarytypename#,#Fastcode#,#Active#,#Displayorder#,#Libraryname#,#Remark#,#Englibraryname#,#Lastupdatedate:DateTime:1/1/0001 12:00:00 AM#,#Uniqueid#)</insert><update id="UpdateDictlibrary" parameterClass="Dictlibrary">UPDATE DICTLIBRARY  SET DICTLIBRARYID=#Dictlibraryid#,LIBRARYTYPECODE=#Librarytypecode#,LIBRARYTYPENAME=#Librarytypename#,FASTCODE=#Fastcode#,ACTIVE=#Active#,DISPLAYORDER=#Displayorder#,LIBRARYNAME=#Libraryname#,REMARK=#Remark#,ENGLIBRARYNAME=#Englibraryname#,LASTUPDATEDATE=#Lastupdatedate#,UNIQUEID=#Uniqueid#WHERE DICTLIBRARYID = #Dictlibraryid#</update><delete id="DeleteDictlibrary" parameterClass="string">DELETE FROM DICTLIBRARYWHERE DICTLIBRARYID = #value#</delete>
</statements>
</sqlMap>
