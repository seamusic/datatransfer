<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="DictdiagnosisMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
<alias>
	<typeAlias alias="Dictdiagnosis"  type="Daan.DataTransfer.Test.Domain.Dictdiagnosis" />
</alias>
<resultMaps>
		<resultMap id="DictdiagnosisResult" class="Dictdiagnosis"><result property="Dictdiagnosisid" column="DICTDIAGNOSISID" type="single" dbType="Real"/><result property="Itemname" column="ITEMNAME" type="string" dbType="varchar"/><result property="Resultsuggestion" column="RESULTSUGGESTION" type="string" dbType="varchar"/><result property="Isdisease" column="ISDISEASE" type="string" dbType="varchar"/><result property="Fastcode" column="FASTCODE" type="string" dbType="varchar"/><result property="Displayorder" column="DISPLAYORDER" type="single" dbType="Real"/><result property="Createdate" column="CREATEDATE" type="DateTime" dbType="DateTime"/><result property="Engitemname" column="ENGITEMNAME" type="string" dbType="varchar"/><result property="Engresultsuggestion" column="ENGRESULTSUGGESTION" type="string" dbType="varchar"/></resultMap>
</resultMaps>
<statements>
	<select id="SelectDictdiagnosis" parameterClass="string" resultMap="DictdiagnosisResult">SELECT DICTDIAGNOSISID AS Dictdiagnosisid,ITEMNAME AS Itemname,RESULTSUGGESTION AS Resultsuggestion,ISDISEASE AS Isdisease,FASTCODE AS Fastcode,DISPLAYORDER AS Displayorder,CREATEDATE AS Createdate,ENGITEMNAME AS Engitemname,ENGRESULTSUGGESTION AS EngresultsuggestionFROM DICTDIAGNOSIS<dynamic prepend="WHERE"><isParameterPresent>DICTDIAGNOSISID = #value#</isParameterPresent></dynamic></select><insert id="InsertDictdiagnosis" parameterClass="Dictdiagnosis">	INSERT INTO DICTDIAGNOSIS (DICTDIAGNOSISID,ITEMNAME,RESULTSUGGESTION,ISDISEASE,FASTCODE,DISPLAYORDER,CREATEDATE,ENGITEMNAME,ENGRESULTSUGGESTION) 	VALUES(#Dictdiagnosisid#,#Itemname#,#Resultsuggestion#,#Isdisease#,#Fastcode#,#Displayorder#,#Createdate:DateTime:1/1/0001 12:00:00 AM#,#Engitemname#,#Engresultsuggestion#)</insert><update id="UpdateDictdiagnosis" parameterClass="Dictdiagnosis">UPDATE DICTDIAGNOSIS  SET DICTDIAGNOSISID=#Dictdiagnosisid#,ITEMNAME=#Itemname#,RESULTSUGGESTION=#Resultsuggestion#,ISDISEASE=#Isdisease#,FASTCODE=#Fastcode#,DISPLAYORDER=#Displayorder#,CREATEDATE=#Createdate#,ENGITEMNAME=#Engitemname#,ENGRESULTSUGGESTION=#Engresultsuggestion#WHERE DICTDIAGNOSISID = #Dictdiagnosisid#</update><delete id="DeleteDictdiagnosis" parameterClass="string">DELETE FROM DICTDIAGNOSISWHERE DICTDIAGNOSISID = #value#</delete>
</statements>
</sqlMap>
