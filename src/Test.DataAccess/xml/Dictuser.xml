<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="DictuserMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
<alias>
	<typeAlias alias="Dictuser"  type="Daan.DataTransfer.Test.Domain.Dictuser" />
</alias>
<resultMaps>
		<resultMap id="DictuserResult" class="Dictuser"><result property="Dictuserid" column="DICTUSERID" type="single" dbType="Real"/><result property="Usercode" column="USERCODE" type="string" dbType="varchar"/><result property="Password" column="PASSWORD" type="string" dbType="varchar"/><result property="Echoname" column="ECHONAME" type="string" dbType="varchar"/><result property="Usernote" column="USERNOTE" type="string" dbType="varchar"/><result property="Active" column="ACTIVE" type="string" dbType="varchar"/><result property="Islock" column="ISLOCK" type="string" dbType="varchar"/><result property="Digitalusbtype" column="DIGITALUSBTYPE" type="string" dbType="varchar"/><result property="Digitalusbid" column="DIGITALUSBID" type="string" dbType="varchar"/><result property="Usertype" column="USERTYPE" type="string" dbType="varchar"/><result property="Allowasname" column="ALLOWASNAME" type="string" dbType="varchar"/><result property="Asname" column="ASNAME" type="single" dbType="Real"/><result property="Dictdepartmentid" column="DICTDEPARTMENTID" type="single" dbType="Real"/><result property="Agreedate" column="AGREEDATE" type="DateTime" dbType="DateTime"/><result property="Issaleman" column="ISSALEMAN" type="string" dbType="varchar"/><result property="Lastupdatedate" column="LASTUPDATEDATE" type="DateTime" dbType="DateTime"/></resultMap>
</resultMaps>
<statements>
	<select id="SelectDictuser" parameterClass="string" resultMap="DictuserResult">SELECT DICTUSERID AS Dictuserid,USERCODE AS Usercode,PASSWORD AS Password,ECHONAME AS Echoname,USERNOTE AS Usernote,ACTIVE AS Active,ISLOCK AS Islock,DIGITALUSBTYPE AS Digitalusbtype,DIGITALUSBID AS Digitalusbid,USERTYPE AS Usertype,ALLOWASNAME AS Allowasname,ASNAME AS Asname,DICTDEPARTMENTID AS Dictdepartmentid,AGREEDATE AS Agreedate,ISSALEMAN AS Issaleman,LASTUPDATEDATE AS LastupdatedateFROM DICTUSER<dynamic prepend="WHERE"><isParameterPresent>DICTUSERID = #value#</isParameterPresent></dynamic></select><insert id="InsertDictuser" parameterClass="Dictuser">	INSERT INTO DICTUSER (DICTUSERID,USERCODE,PASSWORD,ECHONAME,USERNOTE,ACTIVE,ISLOCK,DIGITALUSBTYPE,DIGITALUSBID,USERTYPE,ALLOWASNAME,ASNAME,DICTDEPARTMENTID,AGREEDATE,ISSALEMAN,LASTUPDATEDATE) 	VALUES(#Dictuserid#,#Usercode#,#Password#,#Echoname#,#Usernote#,#Active#,#Islock#,#Digitalusbtype#,#Digitalusbid#,#Usertype#,#Allowasname#,#Asname#,#Dictdepartmentid#,#Agreedate:DateTime:1/1/0001 12:00:00 AM#,#Issaleman#,#Lastupdatedate:DateTime:1/1/0001 12:00:00 AM#)</insert><update id="UpdateDictuser" parameterClass="Dictuser">UPDATE DICTUSER  SET DICTUSERID=#Dictuserid#,USERCODE=#Usercode#,PASSWORD=#Password#,ECHONAME=#Echoname#,USERNOTE=#Usernote#,ACTIVE=#Active#,ISLOCK=#Islock#,DIGITALUSBTYPE=#Digitalusbtype#,DIGITALUSBID=#Digitalusbid#,USERTYPE=#Usertype#,ALLOWASNAME=#Allowasname#,ASNAME=#Asname#,DICTDEPARTMENTID=#Dictdepartmentid#,AGREEDATE=#Agreedate#,ISSALEMAN=#Issaleman#,LASTUPDATEDATE=#Lastupdatedate#WHERE DICTUSERID = #Dictuserid#</update><delete id="DeleteDictuser" parameterClass="string">DELETE FROM DICTUSERWHERE DICTUSERID = #value#</delete>
</statements>
</sqlMap>
